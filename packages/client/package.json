{
  "name": "@cph-scorer/client",
  "version": "1.0.0-alpha.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "bootstrap": "^5.1.1",
    "browser-sync": "^2.27.5",
    "node-sass-package-importer": "^5.3.2",
    "parcel-bundler": "^1.12.5",
    "pug": "^3.0.2",
    "pug-lint": "^2.6.0",
    "rimraf": "^3.0.2",
    "sass": "^1.41.0",
    "standard": "^16.0.4",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "scripts": {
    "compile": "parcel build src/pages/*.pug",
    "watcher": "parcel src/pages/*.pug",
    "server": "cd dist && browser-sync start --server",
    "build": "rimraf dist && yarn compile",
    "pre-commit": "yarn format && yarn test",
    "lint:js": "standard --report stylish --fix src/static/js/**/*.js src/components/**/*.js",
    "lint:css": "stylelint --fix src/static/sass/**/*.scss src/components/**/*.scss",
    "lint:pug": "pug-lint src/layout src/pages src/components",
    "format": "yarn lint:js && yarn lint:css && yarn lint:pug",
    "test": "true"
  },
  "dependencies": {
    "@cph-scorer/model": "^1.0.0-alpha.0",
    "@popperjs/core": "^2.10.2",
    "alpinejs": "^3.4.2",
    "regenerator-runtime": "^0.13.9"
  }
}
